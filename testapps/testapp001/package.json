{
  "name": "testapp001",
  "version": "0.1.0",
  "bin": {
    "testapp001": "bin/testapp001.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "synth": "cdk synth --quiet",
    "synth-debug": "ts-node bin/testapp001.ts",
    "synth-no-ipc": "ts-node bin/testapp001.no-ipc.ts synth --quiet",
    "synth-extra-ipc": "ts-node bin/testapp001.extra-ipc.ts synth --quiet",
    "synth-extra-no-ipc": "ts-node bin/testapp001.extra-no-ipc.ts synth --quiet",
    "hotswap": "cdk deploy --hotswap-fallback --method=direct",
    "hotswap-no-ipc": "ts-node bin/testapp001.no-ipc.ts deploy --hotswap-fallback --method=direct",
    "hotswap-extra-ipc": "ts-node bin/testapp001.extra-ipc.ts deploy --hotswap-fallback --method=direct",
    "hotswap-extra-no-ipc": "ts-node bin/testapp001.extra-no-ipc.ts deploy --hotswap-fallback --method=direct"
  },
  "devDependencies": {
    "@types/jest": "^29.5.4",
    "@types/node": "20.5.3",
    "jest": "^29.6.3",
    "ts-jest": "^29.1.1",
    "aws-cdk": "file:../../packages/aws-cdk",
    "ts-node": "^10.9.1",
    "typescript": "~5.1.6"
  },
  "dependencies": {
    "aws-cdk-lib": "file:../../packages/aws-cdk-lib",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.21"
  }
}
